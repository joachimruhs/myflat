{
  "name": "wsr/myflat",
  "description": "Flat manager, shows bookings of holiday flats in calendars, availability check included.",
  "keywords": ["TYPO3", "extension", "vacation flat", "calendar", "availability check"],
  "homepage": "https://www.web-services-ruhs.de",
  "authors": [
    {
      "name": "Joachim Ruhs",
      "email": "postmaster@joachim-ruhs.de"
    }
  ],
  "license": "MIT",
  "type": "typo3-cms-extension",
  "require": {
	"typo3/cms-core": "^11.5 || ^12.4",
	"typo3/cms-frontend": "^11.5 || ^12.4"
  },
  "replace": {
    "typo3-ter/myflat": "self.version"
  },
  "autoload": {
    "psr-4": {
      "WSR\\Myflat\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
       "extension-key": "myflat"
    }
  }

}
